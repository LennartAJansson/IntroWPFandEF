{
  "ConnectionStrings": {
    "Workloads": "Server=(localdb)\\mssqllocaldb;Database=WorkloadsWpf;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Verbose",
        "System": "Verbose"
      }
    },
    "Using": [ "Serilog.Sinks.File" ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "sample.log",
          "outputTemplate": "{Timestamp:HH:mm:ss}|{MachineName}|{ProcessId}|{ThreadId:00}|{Level:u3}: ({SourceContext}) {Message}{NewLine}{Scope}{NewLine}{Exception}",
          "rollingInterval": "Day",
          "retainedFileCountLimit": "2",
          "shared": "true",
          "flushToDiskInterval": "00:00:01"
        }
      }
    ]
  },
  "Sample": {
    "Production": "This is from appsettings.json"
  }
}
